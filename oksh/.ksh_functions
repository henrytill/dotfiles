acme() {
    tabstop=8 $PLAN9/bin/acme -f $PLAN9/font/lucm/unicode.9.font
}

stripwhitespace() {
    sed -i 's/[[:space:]]\+$//' "$@"
}

cls() {
    if [ -n "$(command -v tput)" ]; then
        tput reset
    else
        clear
    fi
}

