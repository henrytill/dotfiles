Emacs.font:               JetBrains Mono Semi Light:style=Semi Light:pixelsize=12
Emacs.internalBorder:     14
Emacs.menuBar:            off
Emacs.toolBar:            off
Emacs.verticalScrollBars: off

Xft.antialias:            true
Xft.autohint:             false
Xft.dpi:                  96
Xft.hinting:              true
Xft.hintstyle:            hintslight
Xft.lcdfilter:            lcddefault
Xft.rgba:                 rgb

XTerm*termName:           xterm-256color
XTerm*BorderWidth:        14
XTerm*loginShell:         true
XTerm*selectToClipboard:  true
XTerm*geometry:           140x50
XTerm*metaSendsEscape:    true
XTerm*saveLines:          500000
