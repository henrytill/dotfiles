[push]
    default = simple
[alias]
    bpush = !git push origin "$@" && git push backup "$@"
[rebase]
    autosquash = true
[core]
    excludesfile = ~/.gitignore_global
[transfer]
    fsckObjects = true
[fetch]
    fsckObjects = true
[receive]
    fsckObjects = true
