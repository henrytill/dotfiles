[push]
    default = simple
[alias]
    allpush = ! git remote | xargs -L1 git push
[rebase]
    autosquash = true
[core]
    excludesfile = ~/.gitignore_global
[transfer]
    fsckObjects = true
[fetch]
    fsckObjects = true
[receive]
    fsckObjects = true
