[push]
    default = simple
[alias]
    allpush = ! git remote | xargs -L1 git push
[rebase]
    autosquash = true
[core]
    excludesfile = ~/.gitignore_global
    autocrlf = true
[transfer]
    fsckObjects = true
[fetch]
    fsckObjects = true
[receive]
    fsckObjects = true
[commit]
    gpgsign = true
[merge]
    conflictStyle = diff3
