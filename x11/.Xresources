Xft.antialias: true
Xft.autohint: false
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.rgba: rgb

UXTerm*faceName: xft:BerkeleyMono:pixelsize=14
UXTerm*boldFont: xft:BerkeleyMono:style=Bold:pixelsize=14
UXTerm*boldColors: false
UXTerm*colorBDMode: false
UXTerm.termName: xterm-256color
UXTerm.omitTranslation: fullscreen

! https://www.gnu.org/software/emacs/manual/html_node/efaq/Meta-key-does-not-work-in-xterm.html
UXTerm*eightBitInput: false
UXTerm*eightBitOutput: true

! Monokai Pro color scheme
UXTerm*background: #2D2A2E
UXTerm*foreground: #FCFCFA

! Normal colors
UXTerm*color0: #403E41
UXTerm*color1: #FF6188
UXTerm*color2: #A9DC76
UXTerm*color3: #FFD866
UXTerm*color4: #FC9867
UXTerm*color5: #AB9DF2
UXTerm*color6: #78DCE8
UXTerm*color7: #FCFCFA

! Bright colors
UXTerm*color8: #727072
UXTerm*color9: #FF6188
UXTerm*color10: #A9DC76
UXTerm*color11: #FFD866
UXTerm*color12: #FC9867
UXTerm*color13: #AB9DF2
UXTerm*color14: #78DCE8
UXTerm*color15: #FCFCFA

! Local Variables:
! compile-command: "xrdb -load x11/.Xresources"
! End:
